{
  "name": "pacman-html5",
  "description": "Pacman written in TypeScript",
  "version": "3.0.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bobbylight/PacmanJS.git"
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js",
    "watch": "cross-env NODE_ENV=dev webpack serve --config ./webpack/webpack.config.js",
    "start": "electron src/js/electron-main.js",
    "lint": "eslint --ext .ts src/",
    "test": "jest --coverage src/",
    "cover": "cross-env NODE_ENV=test nyc npm test"
  },
  "devDependencies": {
    "@types/jest": "26.0.24",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "copy-webpack-plugin": "9.0.1",
    "cross-env": "7.0.3",
    "css-loader": "5.2.6",
    "electron": "9.4.0",
    "eslint": "7.30.0",
    "html-webpack-plugin": "5.3.2",
    "jest": "27.0.6",
    "mini-css-extract-plugin": "2.1.0",
    "raw-loader": "4.0.2",
    "rimraf": "3.0.2",
    "style-loader": "3.0.0",
    "ts-jest": "27.0.3",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "typescript": "4.3.5",
    "webpack": "5.44.0",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2",
    "webpack-target-electron-renderer": "0.4.0"
  },
  "dependencies": {
    "gtp": "2.0.5"
  }
}
